   "metadata": {},
   "source": [
    "But we want you to know how to calculate it using simple math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "644f3d28",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "78522\n",
      "22587\n"
     ]
    }
   ],
   "source": [
    "num = int(input())\n",
    "total = 0\n",
    "while(num/10):\n",
    "    total*=10\n",
    "    total+=num%10\n",
    "    num=num//10\n",
    "print(total)"
   ]
